              .widget-controls
                a.widget-control-right.widget-remove(href='#', data-toggle='tooltip', data-placement='top', title='Remove Widget')
                  span.fa.fa-times
            // END WIDGET SLIDER
          .col-md-3
            // START WIDGET MESSAGES
            .widget.widget-default.widget-item-icon(onclick="location.href='pages-messages.html';")
