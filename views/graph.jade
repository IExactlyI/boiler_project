        // START WIDGETS
        .row
          .col-md-3
            // START WIDGET SLIDER
            .widget.widget-default.widget-item-icon(onclick="location.href='pages-messages.html';")
              .widget-item-left
                span.fa.fa-bitbucket
              .widget-data
                .widget-int.num-count 48
                .widget-title Total Boiler
