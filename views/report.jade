extends base
block content
    ul.breadcrumb
    .page-content-wrap
        .panel.panel-default
          .panel-heading
            h3.panel-title Steam Flow Rate / Steam Pressure
          .panel-body
            #morris-line2(style='height: 300px;')
    #mb-signout.message-box.animated.fadeIn(data-sound='alert')
        .mb-container
          .mb-middle
            .mb-title
              span.fa.fa-sign-out
              |  Log 
              strong Out
              |  ?
            .mb-content
              p Are you sure you want to log out?
              p Press No if you want to continue work. Press Yes to logout current user.
            .mb-footer
              .pull-right
                a.btn.btn-success.btn-lg(href='pages-login.html') Yes
                button.btn.btn-default.btn-lg.mb-control-close No

